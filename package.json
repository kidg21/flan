{
  "name": "lb-react-core",
  "version": "1.0.0",
  "description": "Repository containing our Styled-Component React widgets",
  "main": "./Bundles/lbreactcore.js",
  "scripts": {
    "storybook": "start-storybook -p 9009 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o ../share/nginx/html",
    "build-storybook-docs": "build-storybook -s public --docs",
    "compileFontAwesome": "webpack --config ./webpackConfigs/fontAwesome.config.js",
    "testBuild": "webpack --config ./webpackConfigs/testBuild.config.js",
    "build": "webpack",
    "test": "mocha",
    "test-coverage": "nyc npm run test",
    "push-coverage": "codacy-coverage --token ca0a1dcce45a40e8bd8fe18863b5c017",
    "testWithCoverage": "npm run test-coverage | npm run push-coverage"
  },
  "nyc": {
    "extension": [
      ".js",
      ".jsx"
    ],
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "**/*.stories.js"
    ],
    "reporter": "text-lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org:DigMap/lbox-react-core.git"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "mapbox-gl": "^1.0.0",
    "node-latest-version": "^1.0.16",
    "prop-types": "^15.7.2",
    "react-intl": "^2.9.0",
    "react-mapbox-gl": "^4.4.0",
    "react-select": "^2.4.3",
    "react-virtualized": "^9.21.1",
    "styled-components": "^4.2.0",
    "styled-normalize": "^8.0.6"
  },
  "devDependencies": {
    "@babel/register": "^7.7.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/free-regular-svg-icons": "^5.8.1",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/pro-light-svg-icons": "^5.8.1",
    "@fortawesome/pro-regular-svg-icons": "^5.8.1",
    "@fortawesome/pro-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@storybook/addon-a11y": "^5.2.6",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-backgrounds": "^5.2.6",
    "@storybook/addon-docs": "^5.2.6",
    "@storybook/addon-info": "^5.2.6",
    "@storybook/addon-knobs": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addon-notes": "^5.2.6",
    "@storybook/addon-options": "^5.2.6",
    "@storybook/addon-storysource": "^5.2.6",
    "@storybook/addon-viewport": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@storybook/theming": "^5.2.6",
    "babel-plugin-markdown": "^0.3.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "chai": "^4.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-cypress": "^2.0.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^15.2.1",
    "lb-react-core": ".",
    "mocha": "^6.2.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-storybook-addon-chapters": "^3.1.6",
    "ss-development": "git+ssh://git@bitbucket.org:DigMap/sstream-development.git",
