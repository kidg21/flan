{
  "env": {
    "development": {
      "plugins": [
        [
          "babel-plugin-styled-components",
          { "ssr": true, "displayName": true, "preprocess": false }
        ]
      ]
    },
    "production": {
      "plugins": [
        [
          "babel-plugin-styled-components",
          { "ssr": true, "displayName": true, "preprocess": false }
        ]
      ]
    }
  },
  "plugins": [
    [
      "babel-plugin-styled-components",
      { "ssr": true, "displayName": true, "preprocess": false }
    ],
    [
      "module-resolver",
      {
        // "root": ["ss-styled-components/src"],
        "alias": {
          "Variables": "ss-styled-components/src/components/base/Variables/Variables.jsx",
          "States": "ss-styled-components/src/components/base/States/States",
          "GlobalStyles": "ss-styled-components/src/components/base/GlobalStyles/GlobalStyles.jsx",
          "css": "ss-styled-components/src/css",
          "fonts": "ss-styled-components/src/fonts",
          "icons": "ss-styled-components/src/icons",
          "images": "ss-styled-components/src/images",
          "helpers": "ss-styled-components/src/components/_helpers",
          "attributes": "ss-styled-components/src/attributes",
          "atoms": "ss-styled-components/src/components/atoms",
          "base": "ss-styled-components/src/components/base",
          "blocks": "ss-styled-components/src/components/blocks",
          "elements": "ss-styled-components/src/components/elements",
          "layout": "ss-styled-components/src/components/layout",
          "templates": "ss-styled-components/src/components/templates",
          "utils": "ss-styled-components/src/components/utils",
          "work": "ss-styled-components/src/components/work",
          ".storybook": "ss-styled-components/.storybook"
        }
      }
    ]
  ]
}
