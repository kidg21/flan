{
  "env": {
    "development": {
      "plugins": [
        [
          "babel-plugin-styled-components",
          { "ssr": true, "displayName": true, "preprocess": false }
        ]
      ]
    },
    "production": {
      "plugins": [
        [
          "babel-plugin-styled-components",
          { "ssr": true, "displayName": true, "preprocess": false }
        ]
      ]
    }
  },
  "plugins": [
    [
      "babel-plugin-styled-components",
      { "ssr": true, "displayName": true, "preprocess": false }
    ],
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "Variables": "./src/components/base/Variables/Variables.jsx",
          "GlobalStyles": "./src/components/base/GlobalStyles/GlobalStyles.jsx",
          "helpers": "./src/components/_helpers",
          "attributes": "./src/attributes",
          "atoms": "./src/components/atoms",
          "base": "./src/components/base",
          "blocks": "./src/components/blocks",
          "layout": "./src/components/layout",
        }
      }
    ]
  ],
  "presets": [
    "es2015", "react", "stage-0"
  ]
}
